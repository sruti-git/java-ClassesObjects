class Student{
    String name;
    int age;
     Student(String name, int age) {   
        this.name = name;             
        this.age = age;
    }
    void study(){
        System.out.println(name+" is studying ");
    }
}
public class Main{
    public static void main(String[] args){
        Student s1=new Student("SRUTI",20);
        s1.study();
    }
}
