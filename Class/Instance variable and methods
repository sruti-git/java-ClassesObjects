class Student{
    String name;
    int age;
    void study(){
        System.out.println(name+" is studying ");
        System.out.println(age+" is studying ");
    }
}
public class Main{
    public static void main(String[] args){
        Student s1=new Student();
        s1.name="Arjun";
        s1.age=20;
        s1.study();
    }
}
