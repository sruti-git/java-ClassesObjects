class Student{
    String name;
    int age;
    Student(){
        name="SRI";
        age=19;
    }
    void study(){
        System.out.println("name: "+name+" age: "+age);
    }
}
public class Main{
    public static void main(String[] args){
        Student s1=new Student();
        s1.study();
    }
}
